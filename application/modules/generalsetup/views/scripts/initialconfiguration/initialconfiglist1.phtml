	function validateform()
	{

		var flag = true;

		var email = dijit.byId('DefaultEmail');
		if(!email.isValid()) {
			flag = false;
		}
		
		if(dijit.byId("noofrowsingrid").value==''){
		    dijit.byId("noofrowsingrid").attr('required','true');
			dijit.byId("noofrowsingrid").focus();
			flag = false;
		}

	    		
		return flag;
		
	}

	function fnValidateForm(){
		var flag = true;

		if(fnValidateTab5() == false){
			ShowThisTab("tab-five");
			flag = false;
		}

		if(fnValidateTab2() == false){
			ShowThisTab("tab-two");
			flag = false;
		}
		
		if(fnValidateTab1() == false){
			ShowThisTab("tab-one");
			flag = false;
		}

		return flag;
	}
