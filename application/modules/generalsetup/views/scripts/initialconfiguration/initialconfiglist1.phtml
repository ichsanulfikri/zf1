	function enablerefcodefields(NoRefCode){ 		
		dijit.byId('RefCodeField1').setAttribute("disabled", true);
		dijit.byId('RefCodeField2').setAttribute("disabled", true);
		dijit.byId('RefCodeField3').setAttribute("disabled", true);
		dijit.byId('RefCodeField4').setAttribute("disabled", true);	
			
		for(loop = 1;loop<=NoRefCode;loop++)
		{			
			var refer = "RefCodeField"+loop;
			dijit.byId(refer).setAttribute("disabled", false);
		}
	}
