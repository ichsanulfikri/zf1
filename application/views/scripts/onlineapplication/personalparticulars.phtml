function AutoFillDropDown(EditableDropDownId,JSONText){
		EditableDropDownId	= document.getElementById(EditableDropDownId);
		var listItems = " ";
		 listItems+= "<option>" + "Select" + "</option>";
		for (var i = 0; i < JSONText.length; i++){
